<%= form_for @book do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :authors %>
  <%= text_field_tag :authors %>

  <%= f.label :publication_year %>
  <%= f.number_field :publication_year %>

  <%= f.label :pages %>
  <%= f.number_field :pages %>

  <%= f.label :cover_image %>
  <%= f.text_field :cover_image %>

  <%= f.submit %>
<% end %>
